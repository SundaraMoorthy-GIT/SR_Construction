              <td style="width: 30px;">{{rowIndex+1}}</td>
                <td > <span class="l1_txt">{{data.Name}} </span>
                  <br><span class="l2_txt">{{data.Receipt_No}} /{{data.AC_Date}} </span> 
                  <br><span class="l4_txt">{{appservice.ago(data.Created_Date)}}  </span> 
                  
